[manager]
# put the ClusterRunner manager host here
# your.windows.manager.host.com

[workers]
# list out all the ClusterRunner worker hosts here
# your.windows.worker.01.com
# your.windows.worker.02.com

[clusterrunner_nodes:children]
manager
workers

[clusterrunner_nodes:vars]
# path of HOME directory on all the VMs
# home=c:\users\your-windows-user

# ClusterRunner manager host, this should be the same as the one under 'manager' inventory group
# manager=your.windows.manager.host.com
